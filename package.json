{
  "name": "gnaf-address-service",
  "version": "0.1.0",
  "description": "Standalone microservice for Australian address validation using G-NAF dataset",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && node -r dotenv/config dist/index.js",
    "dev:watch": "tsc --watch & nodemon dist/index.js",
    "start": "node -r dotenv/config dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts --max-warnings=0",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint:ci": "eslint src/**/*.ts --format=json --output-file=eslint-report.json",
    "migrate": "node scripts/migrate-database.js migrate",
    "migrate:status": "node scripts/migrate-database.js status",
    "migrate:rollback": "node scripts/migrate-database.js rollback",
    "typecheck": "tsc --noEmit",
    "typecheck:ci": "tsc --noEmit --pretty false",
    "audit": "npm audit --audit-level=high",
    "audit:fix": "npm audit fix",
    "security:scan": "./scripts/docker-security-scan.sh",
    "db:setup": "node scripts/setup-database.js",
    "db:migrate": "node scripts/migrate-database.js",
    "gnaf:download": "node scripts/download-gnaf.js",
    "gnaf:import": "node scripts/import-gnaf.js && npm run migrate",
    "gnaf:refresh": "npm run gnaf:download && npm run gnaf:import",
    "gnaf:import-only": "node scripts/import-gnaf.js",
    "test:system": "node scripts/test-system-simple.js",
    "test:smoke": "node scripts/smoke-test.js",
    "docker:build": "docker build -t gnaf-address-service .",
    "docker:build:prod": "docker build -t gnaf-address-service:production --target runtime .",
    "docker:run": "docker run -p 3000:3000 gnaf-address-service",
    "docker:run:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "k8s:deploy": "./scripts/deploy-k8s.sh deploy",
    "k8s:deploy:staging": "./scripts/deploy-k8s.sh deploy staging",
    "k8s:deploy:prod": "./scripts/deploy-k8s.sh deploy production",
    "k8s:rollback": "./scripts/deploy-k8s.sh rollback",
    "k8s:cleanup": "./scripts/deploy-k8s.sh cleanup",
    "ci:full": "npm run lint && npm run typecheck && npm run test:ci && npm run build",
    "ci:prepare": "npm ci && npm run audit",
    "cd:staging": "npm run ci:full && npm run docker:build:prod",
    "cd:production": "npm run ci:full && npm run docker:build:prod && npm run k8s:deploy:prod",
    "monitoring:start": "docker-compose -f docker/elk-stack.yml up -d",
    "monitoring:stop": "docker-compose -f docker/elk-stack.yml down",
    "logs:tail": "docker-compose logs -f gnaf-app"
  },
  "keywords": [
    "gnaf",
    "address-validation",
    "geocoding",
    "spatial",
    "australian-addresses",
    "microservice",
    "real-estate"
  ],
  "author": "Real Estate Commission System",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pg": "^8.11.0",
    "pg-copy-streams": "^6.0.0",
    "prom-client": "^15.1.3",
    "redis": "^4.6.0",
    "stream-transform": "^3.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.0",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "tsx": "^3.12.0",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/gnaf-address-service.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/gnaf-address-service/issues"
  },
  "homepage": "https://github.com/your-org/gnaf-address-service#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageDirectory": "coverage",
    "testTimeout": 30000
  }
}
