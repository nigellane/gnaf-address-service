# G-NAF Address Service Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://gnaf_user:gnaf_password@localhost:5433/gnaf_db

# Import Configuration
GNAF_DATASET_PATH=./data
GNAF_DATASET_URL=https://data.gov.au/data/dataset/geocoded-national-address-file-g-naf
IMPORT_BATCH_SIZE=5000
MAX_CONCURRENT_BATCHES=3

# Database Pool Configuration
DB_POOL_MIN=5
DB_POOL_MAX=20
DB_IDLE_TIMEOUT=30000
DB_CONNECT_TIMEOUT=10000
DB_STATEMENT_TIMEOUT=60000
DB_QUERY_TIMEOUT=30000

# Logging Configuration
LOG_LEVEL=info

# Redis Configuration (Optional)
REDIS_URL=redis://localhost:6379

# Application Configuration
PORT=3001
NODE_ENV=development